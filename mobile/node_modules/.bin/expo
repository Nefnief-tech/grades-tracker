#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/grade-tracker-v2/mobile/node_modules/.pnpm/expo@53.0.9_@babel+core@7.2_5bd1973c68d263c48fbab7f76d165959/node_modules/expo/bin/node_modules:/mnt/f/grade-tracker-v2/mobile/node_modules/.pnpm/expo@53.0.9_@babel+core@7.2_5bd1973c68d263c48fbab7f76d165959/node_modules/expo/node_modules:/mnt/f/grade-tracker-v2/mobile/node_modules/.pnpm/expo@53.0.9_@babel+core@7.2_5bd1973c68d263c48fbab7f76d165959/node_modules:/mnt/f/grade-tracker-v2/mobile/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/grade-tracker-v2/mobile/node_modules/.pnpm/expo@53.0.9_@babel+core@7.2_5bd1973c68d263c48fbab7f76d165959/node_modules/expo/bin/node_modules:/mnt/f/grade-tracker-v2/mobile/node_modules/.pnpm/expo@53.0.9_@babel+core@7.2_5bd1973c68d263c48fbab7f76d165959/node_modules/expo/node_modules:/mnt/f/grade-tracker-v2/mobile/node_modules/.pnpm/expo@53.0.9_@babel+core@7.2_5bd1973c68d263c48fbab7f76d165959/node_modules:/mnt/f/grade-tracker-v2/mobile/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/expo@53.0.9_@babel+core@7.2_5bd1973c68d263c48fbab7f76d165959/node_modules/expo/bin/cli" "$@"
else
  exec node  "$basedir/../.pnpm/expo@53.0.9_@babel+core@7.2_5bd1973c68d263c48fbab7f76d165959/node_modules/expo/bin/cli" "$@"
fi
